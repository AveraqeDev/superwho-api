{
  "name": "superwho-api",
  "version": "0.1.0",
  "description": "Backend api for SuperWho? application",
  "main": "src/server.js",
  "scripts": {
    "lint": "eslint \"src/**/*.js\"",
    "migrate": "postgrator --config postgrator-config.js",
    "migrate:test": "env NODE_ENV=test npm run migrate",
    "seed": "psql -U postgres -d superwho -f ./db/seeds/seed.all_tables.sql",
    "seed:test": "psql -U postgres -d superwho_test -f ./db/seeds/seed.all_tables.sql",
    "test": "npm run lint && mocha --require test/setup.js",
    "dev": "nodemon src/server.js",
    "start": "node src/server.js"
  },
  "keywords": [],
  "author": "Matthew Wagaman <averaqedev@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.25.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^14.3.2",
    "express": "^4.17.2",
    "helmet": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "knex": "^1.0.1",
    "morgan": "^1.10.0",
    "pg": "^8.7.1",
    "redis": "^4.0.2"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "eslint": "^8.7.0",
    "mocha": "^9.2.0",
    "nodemon": "^2.0.15",
    "postgrator-cli": "^4.0.0",
    "supertest": "^6.2.2"
  }
}
